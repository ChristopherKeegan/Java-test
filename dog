class Dog{
  int age;
  
  public Dog(int dogsAge){
    age = dogsAge;
  }
  
  public void bark(){
    System.out.println("Bark");
  }
  
  public void run(int feet){
    System.out.prinln("Your dog ran " + feet + " feet!");
  }
  
  public int getAge(){
    return age;
  }
  
  public static void main(String[] args){
    Dog spike = new Dog(13);
    spike.bark();
    spike.run(230);
    int spikeAge = spike.getAge();
    System.out.prinln(spikeAge);
  }
}
